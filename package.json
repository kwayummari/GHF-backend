{
    "name": "express-backend",
    "version": "1.0.0",
    "description": "Scalable Express.js backend application",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest --coverage",
        "lint": "eslint .",
        "migrate": "sequelize-cli db:migrate",
