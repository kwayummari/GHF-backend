        "cors": "^2.8.5",
        "dotenv-flow": "^3.2.0",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "express-validator": "^7.0.1",
        "helmet": "^7.0.0",
        "http-status-codes": "^2.2.0",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.3.3",
        "node-schedule": "^2.1.1",
        "nodemailer": "^6.9.3",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "pdfkit": "^0.13.0",
        "sequelize": "^6.32.0",
        "swagger-autogen": "^2.23.1",
        "swagger-jsdoc": "^6.2.8",
